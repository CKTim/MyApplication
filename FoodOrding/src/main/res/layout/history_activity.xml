<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white">

    <include
        android:id="@+id/custom_mylove_actionbar"
        layout="@layout/custom_mylove_actionbar" />

    <ListView
        android:id="@+id/myhistory_lv"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/custom_mylove_actionbar"
        android:layout_marginTop="10dp"
        android:divider="@color/lightorange"
        android:dividerHeight="15dp"></ListView>

</RelativeLayout>